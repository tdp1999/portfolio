{
  "name": "@portfolio/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.16.0",
  "scripts": {
    "start": "nx serve landing",
    "dev:landing": "nx serve landing",
    "dev:api": "nx serve api --no-tui",
    "dev:console": "nx serve console --port=4300",
    "dev": "nx run-many -t serve --projects=landing,api --parallel=2",
    "build:landing": "nx build landing",
    "build:api": "nx build api",
    "build": "nx run-many -t build --projects=landing,api",
    "test:landing": "nx test landing",
    "test:api": "nx test api",
    "test": "nx run-many -t test --all",
    "test:watch": "nx test landing --watch",
    "test:coverage": "nx run-many -t test --all --coverage",
    "test:e2e": "nx e2e landing-e2e",
    "test:e2e:headed": "nx e2e-headed landing-e2e",
    "test:e2e:ui": "nx e2e-ui landing-e2e",
    "lint:landing": "nx lint landing",
    "lint:api": "nx lint api",
    "lint": "nx run-many -t lint --all",
    "lint:fix": "nx run-many -t lint --all --fix",
    "format:check": "prettier --check .",
    "graph": "nx graph",
    "affected:test": "nx affected -t test",
    "affected:build": "nx affected -t build",
    "affected:lint": "nx affected -t lint",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d",
    "db:studio": "prisma studio --url=postgresql://postgres:postgres@localhost:5432/portfolio",
    "prisma:generate": "prisma generate --config=apps/api/prisma/prisma.config.ts",
    "prisma:migrate:dev": "prisma migrate dev --config=apps/api/prisma/prisma.config.ts",
    "prisma:migrate:deploy": "prisma migrate deploy --config=apps/api/prisma/prisma.config.ts",
    "prisma:studio": "prisma studio --config=apps/api/prisma/prisma.config.ts",
    "prepare": "husky"
  },
  "private": true,
